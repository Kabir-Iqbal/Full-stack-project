{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    },
    {
      "buildCommand": "npm install --legacy-peer-deps && npm run build",
      "outputDirectory": "dist"
    }
    
  ]
}